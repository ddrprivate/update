	</head>
	<body class="landing">
	
		<div id="page-wrapper">
			<div itemscope itemtype= "http://schema.org/ItemPage">
			
				
			<!-- Header -->
				<header id="header">
					<h1 id="logo"><a href="/">Dealer de Rimes | <em>Source d'inspiration</a></h1>
					<nav id="nav">
						<ul>
							<li><a href="/">Rimez</a></li>
							<li><a href="/a-propos">À propos</a></li>
							<li><a href="/inscription" class="button special">Sign Up</a></li>
						</ul>
					</nav>
				</header>

			<!-- Main -->
			<section id="one" class="wrapper style1">
					<div class="container">
						<header class="major">
							<h3 itemprop="name">Dictionnaire de rimes</h3>
								<form method="post" action="/#dictionnaire">
									<div class="row uniform 50%">
										<div class="12u$">
											<input type="text" name="mot" id="name" value="<%=@mot%>" placeholder="<%=@mot%>" />
										</div>
									
										<% if @radio == 1 %>
										<div class="6u 12u$(medium)">
											<input type="radio" id="rimeo_riche" name="rimeo" value="1" checked>
											<label for="rimeo_riche">Rimes riches</label>
										</div>
										<div class="6u 12u$(medium)">
											<input type="radio" id="rimeo_pauvre" name="rimeo" value="0">
											<label for="rimeo_pauvre">Rimes pauvres (plus de suggestions)</label>
										</div>
										<% else %>
											<div class="6u 12u$(medium)">
											<input type="radio" id="rimeo_riche" name="rimeo" value="1">
											<label for="rimeo_riche">Rimes riches</label>
										</div>
										<div class="6u 12u$(medium)">
											<input type="radio" id="rimeo_pauvre" name="rimeo" value="0" checked>
											<label for="rimeo_pauvre">Rimes pauvres (plus de suggestions)</label>
										</div>
										<% end %>
										<div class="12u$">
											<ul class="actions">
												<li><input type="submit" value="Rime" class="special" /></li>
											</ul>
									</div>
								</form>
						</header>

						<!-- Content -->
						<div itemscope itemtype="http://schema.org/DataCatalog">
								<h4 itemprop="name"><%=@mot%></h4>
								<ul class="icons">
								<h5><%=@asso1.first%></h5>
								
								<%@asso1=@asso1.drop(1) unless @asso1.empty?%>
								<%@asso1.each do |x|%>
								<li><a href="/dictionnaire/<%=x.force_encoding("UTF-8")%>" style="color: rgba(255, 255, 255, 0.75" itemprop="url"> <%=x.force_encoding("UTF-8") + "  "%></a></li>
								<% end %>
								<h5></h5>
								&nbsp
								<h5><%=@asso2.first%></h5>
								<%@asso2=@asso2.drop(1) unless @asso2.empty?%>
								<%@asso2.each do |x|%>
								<li><a href="/dictionnaire/<%=x.force_encoding("UTF-8")%>" style="color: rgba(255, 255, 255, 0.75" itemprop="url"> <%=x.force_encoding("UTF-8") + "  "%></a></li>
								<% end %>
								<h5></h5>
								&nbsp
								<h5><%=@asso3.first%></h5>
								<%@asso3=@asso3.drop(1) unless @asso3.empty?%>
								<%@asso3.each do |x|%>
								<li><a href="/dictionnaire/<%=x.force_encoding("UTF-8")%>" style="color: rgba(255, 255, 255, 0.75" itemprop="url"> <%=x.force_encoding("UTF-8") + "  "%></a></li>
								<% end %>
								<h5></h5>
								&nbsp
								<h5><%=@asso4.first%></h5>
								<%@asso4=@asso4.drop(1) unless @asso4.empty?%>
								<%@asso4.each do |x|%>
								<li><a href="/dictionnaire/<%=x.force_encoding("UTF-8")%>" style="color: rgba(255, 255, 255, 0.75" itemprop="url"> <%=x.force_encoding("UTF-8") + "  "%></a></li>
								<% end %>
								<h5></h5>
								&nbsp
								<h5><%=@asso5.first%></h5>
								<%@asso5=@asso5.drop(1) unless @asso5.empty?%>
								<%@asso5.each do |x|%>
								<li><a href="/dictionnaire/<%=x.force_encoding("UTF-8")%>" style="color: rgba(255, 255, 255, 0.75" itemprop="url"> <%=x.force_encoding("UTF-8") + "  "%></a></li>
								<% end %>
								</ul>
								<center><a href="/a-propos"> Comment ça marche ? </a></center>
						</div>	
					</div>
				</div>
					
				</section>

			<!-- Footer -->
				<footer id="footer">
					<ul class="icons">
						<li><a href="https://twitter.com/Dealerderimes" class="icon alt fa-twitter"><span class="label">Twitter</span></a></li>
						<li><a href="https://www.facebook.com/dealerderime" class="icon alt fa-facebook"><span class="label">Facebook</span></a></li>
