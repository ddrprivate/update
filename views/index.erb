	<body class="landing">
		<div id="page-wrapper">
			<div itemscope itemtype= "http://schema.org/WebSite">

			<!-- Header -->
				<header id="header">
					<h1 id="logo"><a href="/">Dealer de Rimes | <em>Source d'inspiration</a></h1>
					<nav id="nav">
						<ul>
							<li><a href="/">Dictionnaire de rimes</a></li>
							<li><a href="/a-propos">À propos</a></li>
							<li><a href="/inscription" class="button special">Sign Up</a></li>
						</ul>
					</nav>
				</header>

			<!-- Banner -->
				<section id="banner">
					<div class="content">
						<header>
							<h2 itemprop="name">Dealer de Rimes</h2>
							<p><strong>Trouvez l’inspiration avec notre dictionnaire de rimes et allitérations.</strong><br>Il contient les mots et expressions les plus utilisés du Rap Français.<br><br>
							   Entrez un mot et notre algorithme vous proposera les meilleurs rimes<br> en fonction des <a href="/a-propos">allitérations</a> trouvées.</p>
						</header>
						<span class="image" itemprop="image"><img src="images/dictionnaire_de_rimes_logo.jpg" alt="dealerderimes logo" style="max-width: 100%; height: auto;"/></span>
					</div>
					<a href="#dictionnaire" class="goto-next scrolly">Next</a>
				</section>

			<!-- One -->
				<section id="dictionnaire" class="wrapper style1">
					<div class="container">
						<header class="major">
							<h3>Dictionnaire de rimes et allitérations</h3>
								<form method="post" action="/#dictionnaire">
									<div class="row uniform 50%">
										<div class="12u$">
											<% if @mot == 0%>
											<input type="text" name="mot" id="name" value="" placeholder="Entrez un mot" />
											<% else%>
											<input type="text" name="mot" id="name" value="<%=@mot%>" placeholder="<%=@mot%>" />
											<% end %>
											
										</div>
										
										<% if @radio == 1 %>
										<div class="6u 12u$(medium)">
											<input type="radio" id="rimeo_riche" name="rimeo" value="1" checked>
											<label for="rimeo_riche">Rimes riches</label>
										</div>
										<div class="6u 12u$(medium)">
											<input type="radio" id="rimeo_pauvre" name="rimeo" value="0">
											<label for="rimeo_pauvre">Rimes pauvres (plus de suggestions)</label>
										</div>
										<% else %>
										<div class="6u 12u$(medium)">
											<input type="radio" id="rimeo_riche" name="rimeo" value="1">
											<label for="rimeo_riche">Rimes riches</label>
										</div>
										<div class="6u 12u$(medium)">
											<input type="radio" id="rimeo_pauvre" name="rimeo" value="0" checked>
											<label for="rimeo_pauvre">Rimes pauvres (plus de suggestions)</label>
										</div>
										<% end %>
										<div class="12u$">
											<ul class="actions">
												<li><input type="submit" value="Rime" class="special" /></li>
											</ul>
									</div>
								</form>
						</header>

						<!-- Content -->
								
								<% if @mot!=0 %>
								<h4><%=@mot%></h4>
								<%end%>
								<ul class="icons">
								<% if @asso1.empty?%>
								
								<%else%>
								
								<h5><%=@asso1.first%></h5>
								<%@asso1=@asso1.drop(1) unless @asso1.empty?%>
								<%@asso1.each do |x|%>
								<li><a href="/dictionnaire/<%=x.force_encoding("UTF-8")%>" style="color: rgba(255, 255, 255, 0.75"> <%=x.force_encoding("UTF-8") + "  "%></a></li>
								<% end %>
								&nbsp
								<h5></h5>
								<h5><%=@asso2.first%></h5>
								<%@asso2=@asso2.drop(1) unless @asso2.empty?%>
								<%@asso2.each do |x|%>
								<li><a href="/dictionnaire/<%=x.force_encoding("UTF-8")%>" style="color: rgba(255, 255, 255, 0.75"> <%=x.force_encoding("UTF-8") + "  "%></a></li>
								<% end %>
								&nbsp
								<h5></h5>
								<h5><%=@asso3.first%></h5>
								<%@asso3=@asso3.drop(1) unless @asso3.empty?%>
								<%@asso3.each do |x|%>
								<li><a href="/dictionnaire/<%=x.force_encoding("UTF-8")%>" style="color: rgba(255, 255, 255, 0.75"> <%=x.force_encoding("UTF-8") + "  "%></a></li>
								<% end %>
								<h5></h5>
								&nbsp
								<h5><%=@asso4.first%></h5>
								<%@asso4=@asso4.drop(1) unless @asso4.empty?%>
								<%@asso4.each do |x|%>
								<li><a href="/dictionnaire/<%=x.force_encoding("UTF-8")%>" style="color: rgba(255, 255, 255, 0.75"> <%=x.force_encoding("UTF-8") + "  "%></a></li>
								<% end %>
								<h5></h5>
								&nbsp
								<h5><%=@asso5.first%></h5>
								<%@asso5=@asso5.drop(1) unless @asso5.empty?%>
								<%@asso5.each do |x|%>
								<li><a href="/dictionnaire/<%=x.force_encoding("UTF-8")%>" style="color: rgba(255, 255, 255, 0.75"> <%=x.force_encoding("UTF-8") + "  "%></a></li>
								<% end %>
								</ul>
								<%end%>
								<center>
							 <ul class="icons">
							 <p><strong>Exemples:</strong></p>
							<li>Rimes avec:<a href="/dictionnaire/bicrave" itemprop="url"> Bicrave</a></li>
							<li>Rimes avec:<a href="/dictionnaire/qualité" itemprop="url"> Qualité</a></li>  
							</ul>
							 </center>
								<center itemprop="about"><a href="/a-propos" itemprop="url"> Comment Dealer de rimes fonctionne ? </a></center>
							
							

